<div class="container" ng-show="timer.data.value == 0 && scorings.total == 0 ">
	<div class="jumbotron">
			<h1>Aguardando...</h1>
			<p>Para começar a pontuar, inicie o cronômetro</p>
		</div>
	</div>
</div>

<div class="container" ng-hide="timer.data.value == 0 && scorings.total == 0 ">
	<!-- SALAS -->
	<div class="row">

		<div class="col-md-6 col-sm-6"
			ng-init="k = 'first'">

			<div class="thumbnail">
				<h2>Sala 1</h2>

				<div class="btn-group btn-group-justified">
					<label class="btn btn-danger" ng-model="scoreData.rooms['first']"
						btn-radio="0">Nop</label>
					<label class="btn btn-primary" ng-model="scoreData.rooms['first']"
						btn-radio="1">1st</label>
					<label class="btn btn-primary" ng-model="scoreData.rooms['first']"
						btn-radio="2">2nd</label>
					<label class="btn btn-primary" ng-model="scoreData.rooms['first']"
						btn-radio="3">3rd</label>
					<label class="btn btn-info" ng-model="scoreData.rooms['first']"
						btn-radio="4">4th</label>
					<span class="input-group-addon">{{scorings.rooms['first'] || 0}} pts</span>
				</div>

				<h5>Obstáculos</h5>
				<div class="btn-group btn-group-justified">
					<label class="btn btn-info"
						ng-model="scoreData.obstacles[j]"
						ng-repeat="j in [1,2]"
						btn-checkbox>
							{{$index + 1}}
					</label>
					<span class="input-group-addon">{{sumSpecial(scorings.obstacles, [1,2])}} pts</span>
				</div>

				<h5>Gaps</h5>
				<div class="btn-group btn-group-justified">
					<label class="btn btn-warning"
						ng-model="scoreData.gaps[k]"
						ng-repeat="k in [1,2]"
						btn-checkbox>
							{{$index + 1}}
					</label>
					<span class="input-group-addon">{{sumSpecial(scorings.gaps, [1,2])}} pts</span>
				</div>

				<h5>Redutores</h5>
				<div class="btn-group btn-group-justified">
					<label class="btn btn-danger"
						ng-model="scoreData.speedbump[j]"
						ng-repeat="j in [1,2]"
						btn-checkbox>
							{{$index + 1}}
					</label>
					<span class="input-group-addon">{{sumSpecial(scorings.speedbump, [1,2])}} pts</span>
				</div>

				<h5>Interceções</h5>
				<div class="btn-group btn-group-justified">
					<label class="btn btn-success"
						ng-model="scoreData.intersection[k]"
						ng-repeat="k in [1,2]"
						btn-checkbox>
							{{$index + 1}}
					</label>
					<span class="input-group-addon">{{sumSpecial(scorings.intersection, [1,2])}} pts</span>
				</div>
			</div>
		</div>


		<div class="col-md-6 col-sm-6"
			ng-init="k = 'secc'">

			<div class="thumbnail">
				<h2>Sala 2</h2>

				<div class="btn-group btn-group-justified">
					<label class="btn btn-danger" ng-model="scoreData.rooms['secc']"
						btn-radio="0">Nop</label>
					<label class="btn btn-primary" ng-model="scoreData.rooms['secc']"
